<extension xmlns="http://ns.adobe.com/air/extension/23.0">
  <id>com.marpies.ane.facebook.accountkit</id>
  <versionNumber>1.2.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libAccountKit.a</nativeLibrary>
            <initializer>AccountKitInitializer</initializer>
            <finalizer>AccountKitFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libAccountKit.jar</nativeLibrary>
            <initializer>com.marpies.ane.facebook.accountkit.AccountKitExtension</initializer>
            <finalizer>com.marpies.ane.facebook.accountkit.AccountKitExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libAccountKit.jar</nativeLibrary>
            <initializer>com.marpies.ane.facebook.accountkit.AccountKitExtension</initializer>
            <finalizer>com.marpies.ane.facebook.accountkit.AccountKitExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
